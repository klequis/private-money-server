{
  "name": "private-money-server",
  "version": "0.0.1",
  "description": "Server for Private Money app.",
  "main": "index.js",
  "scripts": {
    "build": "npm run-script delete-dist && npm run-script transcompile && npm run-script copy && npm run-script install-prod",
    "copy": "cp package.json dist/package.json",
    "delete-dist": "rm -rf dist",
    "install-prod": "cd dist && npm i --production",
    "lint": "eslint *.*",
    "start": "NODE_ENV=development NODE_PATH=./ nodemon --exec babel-node server/index.js",
    "start-dev": "DEBUG=server* NODE_ENV=development NODE_PATH=./ nodemon --exec babel-node server/index.js",
    "start-prod": "NODE_ENV=production NODE_PATH=./ nodemon --exec babel-node server/index.js",
    "test": "NODE_ENV=testLocal NODE_PATH=./ mocha --require @babel/register",
    "test-coverage": "NODE_ENV=testLocal NODE_PATH=./ nyc mocha --require @babel/register",
    "test-watch": "export WATCH='watch' && nodemon --exec 'npm test'",
    "transcompile": "babel . -d dist --ignore node_modules --source-maps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.17.0",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "helmet": "^4.4.1",
    "json2csv": "^5.0.6",
    "mongodb": "^3.6.4",
    "morgan": "^1.10.0",
    "ramda": "^0.27.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/register": "^7.12.13",
    "@babel/types": "^7.12.17",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "supports-color": "^7.2.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
